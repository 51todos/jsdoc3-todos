{
  "tags": {
	"allowUnknownTags": true
  },
  "source": {
	"include": ["src/jaguarjs", "README.md"],
	"exclude": [],
	"includePattern": ".+\\.js(doc|x)?$",
	"excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
	"cleverLinks": false,
	"monospaceLinks": false,
	"default": {
	  "outputSourceFiles" : true
	},
	"applicationName": "Assets.js",
	"meta": {
	  "title": "Assets.js",
	  "description": "Assets.js - API Document",
	  "keyword": "Assets.js"
	},
	"linenums": false
  },
  "opts": {
	"template": "templates/jaguarjs",
	"encoding": "utf8",
	"destination": "docs/jaguarjs",
	"recurse": true
  },
  "markdown": {
	"parser": "gfm",
	"hardwrap": true
  }
}
