{
  "tags": {
	"allowUnknownTags": true
  },
  "source": {
	"include": ["src", "README.md"],
	"exclude": [],
	"includePattern": ".+\\.js(doc|x)?$",
	"excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
	"cleverLinks": false,
	"monospaceLinks": false,
	"default": {
	  "outputSourceFiles" : true
	},
	"applicationName": "Assets.js",
	"openGraph": {
	  "title": "",
	  "type": "website",
	  "image": "",
	  "site_name": "",
	  "url": ""
	},
	"meta": {
	  "title": "",
	  "description": "",
	  "keyword": ""
	},
	"linenums": true
  },
  "opts": {
	"template": "templates/jaguarjs",
	"encoding": "utf8",
	"destination": "./docs",
	"recurse": true,
	"query": "value",
	"private": true,
	"lenient": true
  },
  "markdown": {
	"parser": "gfm",
	"hardwrap": true
  }
}
